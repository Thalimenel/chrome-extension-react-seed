{
  "name": "chrome-extension-react-seed",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "start": "react-scripts start",
    "clean_build": "rimraf build",
    "react_build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "background_build": "gulp build_background",
    "order_after_build": "mkdir build/popup; mv build/index.html build/popup/popup.html",
    "update_manifest_version": "gulp update_manifest_ver",
    "build": "run-s clean_build react_build background_build order_after_build update_manifest_version",
    "pack": "gulp pack",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "./tsconfig.json"
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "homepage": "../",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/adm-zip": "^0.5.5",
    "@types/chrome": "^0.0.277",
    "@types/glob": "^8.1.0",
    "@types/gulp": "^4.0.17",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/parser": "^8.8.1",
    "adm-zip": "^0.5.16",
    "gulp": "^5.0.0",
    "gulp-replace": "^1.1.4",
    "react-scripts": "^5.0.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "webpack-cli": "^5.1.4",
    "yarn-run-all": "^3.1.1"
  }
}
